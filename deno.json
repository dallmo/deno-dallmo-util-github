{
  "name": "@dallmo/util-github",
  "description": "deno module, functions making use of the github API",
  "version": "0.3.3",
  "tasks": {
    "dev": "deno run --watch app.ts",
    "test": "cd ./test; deno test --allow-read",
    "test-jsr": "cd ./test; deno run --allow-env --allow-net ./test-via-jsr.ts",
    "test-local": "cd ./test; deno run --allow-env --allow-net ./test-via-local.ts",
    "update-cache": "deno cache --reload etc/deps.ts",
    "check-all": "deno check app.ts mod.ts etc/deps.ts test/test.ts"
  },
  "exports": "./mod.ts"
}
